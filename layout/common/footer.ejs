<footer id="footer">
    <div class="container">
        <div class="container-inner">
            <a id="back-to-top"><i class="icon fa fa-angle-up"></i></a>
            <div class="credit">
                <h1 class="logo-wrap">
                    <a href="<%- url_for() %>" class="logo"></a>
                </h1>
                <p>&copy; <%= date(new Date(), 'YYYY') %> <%= config.author || config.title %></p>
                <% if (theme.customize.filing !== '') { %>
                    <p><%= theme.customize.filing %></p>
                <% } %>
            </div>
        </div>
    </div>
</footer>
