.timeline-wrap
    padding: 60px
    h1
        border-bottom: 1px solid #eee
        padding-bottom: 20px
        margin-bottom: 20px
        text-align: center
        font-size: 30px
        font-weight: bold
    .archive-list-item
        font-size: 20px
        text-align: center
        a
            text-decoration: none
            font-size: 100%
            font-weight: bold
            color: #333
        a:hover
            text-decoration: underline
        .archive-list-count
            font-size: 100%
